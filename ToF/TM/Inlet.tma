%{
int z = 31;
int loop_i;
int ambi;
int dfi;
%}

State Loop_idle {}

State Loop_init_sdc {
   { loop_i = 0;
	Validate Loop_step_dc;
    }
}

State Loop_init_sdnc {
	{ loop_i = 0;
	  Validate Loop_step_dnc;
        }
}

State Loop_init_suc {
	{ loop_i = 0;
	  Validate Loop_step_uc;
	}
}

State Loop_init_sunc {
	{ loop_i = 0;
	  Validate Loop_step_unc;
	}
}

State Loop_step_dc {

	{
	dfi = 1025;
	ambi = 1000;

	 if ( ++loop_i > 33 ) {
		Validate Loop_idle;
		} else {
		 int stp, stp2;
		 stp = dfi + ( loop_i - 1 )*z*.9;
		 stp2 = ambi - ( loop_i - 1 )*z;
		 ci_sendfcmd(0,"set ambient zero flow %d sccm\n",stp2);
		}
	}
	+15 Validate Loop_step_dc;
}

State Loop_step_dnc {

	{
	dfi = 1085;
	ambi = 650;

	if ( ++loop_i > 33 ) {
		Validate Loop_idle;
		} else {
		  int stp, stp2;
		  stp = dfi + ( loop_i - 1 )*z*0.90;
		  stp2 = ambi - ( loop_i -1 )*z;
		  ci_sendfcmd(0,"set ambient zero flow %d sccm\n",stp2);
		  ci_sendfcmd(0,"set dilution flow %d sccm\n",stp);
		}
	}
	+15 Validate Loop_step_dnc;
}

State Loop_step_uc {

	{
	dfi = 1470;
	ambi = 170;

	if ( ++loop_i > 33 ) {
		Validate Loop_idle;
		} else {
		  int stp, stp2;
		  stp = dfi - ( loop_i - 1 )*z*0.95;
		  stp2 = ambi + ( loop_i - 1 )*z;
		  ci_sendfcmd(0,"set dilution flow %d sccm\n",stp);
		  ci_sendfcmd(0,"set ambient zero flow %d sccm\n",stp2);
		}
	}
	+15 Validate Loop_step_uc;
}

State Loop_step_unc {

	{

	dfi = 1530;
	ambi = 0;
	if ( ++loop_i > 33 ) {
		Validate Loop_idle;
		} else {
		  int stp, stp2;
		  stp = dfi - ( loop_i - 1 )*z*.95;
		  stp2 = ambi + ( loop_i -1 )*z;
		  ci_sendfcmd(0,"set ambient zero flow %d sccm\n",stp2);
		}
	}
	+15 Validate Loop_step_unc;
}

