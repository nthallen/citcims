IG {
  HBox{ +-; Title: "Ion Gauges"; -+ };
  Hex_P: (Hex_P,9) mbar;
  ToF_P: (ToF_P,9) mbar;
  Stat:  (IG_stat,3);
  Stale: (IG_stale,3);
}

Flows {
  HBox{ +-; Title: "Flows"; -+ };
  ""       Set     Flow;
  MFC_S1: (MFC_S1_sp,6) (MFC_S1,6) slm;
  MFC_S2: (MFC_S2_sp,6) (MFC_S2,6) sccm;
  ZF2_F:  (ZF2_F_sp,6)  (ZF2_F,6) sccm;
  Tot_F:  (Tot_F_sp,6)  (Tot_F,6) slm;
}
